@model Core.Models.AppUser


@{
    ViewBag.Title = "Approve/Reject Page";
}

<div class="panel panel-primary">
    <div class="panel-heading">User @Model.FirstName @Model.SecondName @Model.Patronymic</div>
    <table class="table table-striped">
        <tr>
            <th>Property</th>
            <th>User</th>
        </tr>
        <tr>
            <td>ID</td>
            <td>@Model.Id</td>
        </tr>
        <tr>
            <td>Email/Account Name</td>
            <td>@Model.UserName</td>
        </tr>
        <tr>
            <td>Gender</td>
            <td>@Model.Gender</td>
        </tr>
        <tr>
            <td>Place Of Work</td>
            <td>@Model.Workplace</td>
        </tr>
        <tr>
            <td>Job Position</td>
            <td>@Model.JobPosition</td>
        </tr>
        <tr>
            <td>Country</td>
            <td>@Model.Country</td>
        </tr>
        <tr>
            <td>City</td>
            <td>@Model.City</td>
        </tr>
        <tr>
            <td>Password</td>
            <td>@Model.RawPassword</td>
        </tr>
    </table>
</div>
@Html.ActionLink("Back", "Index", null, new { @class = "btn btn-default" })

